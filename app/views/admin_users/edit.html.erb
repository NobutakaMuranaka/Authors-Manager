<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <%= form_for(@admin_user) do |f| %>
      <%= render 'shared/error_messages', object: f.object %>
    <div class = 'form-group'>
        <%= f.label '名前' %>
        <%= f.text_field :name, class: 'form-control', id: 'admin_user_name' %>
      </div>
    <div class = 'form-group'>
      <%= f.label :email %>
      <%= f.text_field :email, class: 'form-control', id: 'admin_user_email' %>
    </div>
    <%= f.submit "更新する", class: "btn btn-primary" %>
    <% end %>
  </div>
</div>